#!/bin/sh

FILENAME='csv_filename=digital@1143___6_02_2023'


# clear

# printf "\n\nCSV \n\n"
make --quiet -C ~/libs/csv

# printf "\n\nPARSE_LCD \n\n"
# make --quiet RSYNC

if [ ! -d build ] ; then
    cmake -S . -B build -DCMAKE_BUILD_TYPE=DEBUG
fi

cmake --build build --parallel 2
rc=$?

if [ $rc -ne 0 ] ; then
    set-title Error
    exit 1
fi

if [ ! -r parseSaleae ] ; then
    ln -s build/parseSaleae
fi

make --quiet $FILENAME gnss.lst

date
