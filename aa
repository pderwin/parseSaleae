#!/bin/sh
# clear

# printf "\n\nCSV \n\n"
make --quiet -C ~/libs/csv

# printf "\n\nPARSE_LCD \n\n"
make --quiet RSYNC

cmake -S . -B build -DCMAKE_BUILD_TYPE=DEBUG

cmake --build build
rc=$?

if [ $rc -ne 0 ] ; then
    set-title Error
    exit 1
fi

if [ ! -r parseSaleae ] ; then
    ln -s build/parseSaleae
fi

make --quiet digital.lst

date
